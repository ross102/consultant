           <span class="spinner-rotate"></span>
           
      </div>
 </section>

 
     <!-- HOME -->
     <section id="home">
        <div class="row">
                  <div class="owl-carousel owl-theme home-slider">
                       <div class="item item-first">
                            <div class="caption">
                                 <div class="container">
                                      <div class="col-md-12 col-sm-12">
                                         <h1> Write Your Dreams Down</h1>
                                         <h2>Dreams And Nightmares</h2>
                                         <a href="/post" class="section-btn btn btn-default smoothScroll">view  more</a>
                                      </div>
                                 </div>
                            </div>
                       </div>
                       <div class="item item-second">
                            <div class="caption">
                                 <div class="container">
                                      <div class="col-md-12 col-sm-12">
                                           <h1>Your Idea Is An Answer</h1>
                                           <h3>Together We Are challengingly Creative</h3>
                                           <a href="/postBus" class="section-btn btn btn-default smoothScroll">Request for a consultant</a>
                                      </div>
                                 </div>
                            </div>
                       </div>
                       <div class="item item-third">
                            <div class="caption">
                                 <div class="container">
                                      <div class="col-md-12 col-sm-12">
                                           <h1>Bringing Ideas To Life</h1>
                                           <h3> SeaOfApps - Minning Ideas  </h3>
                                           <a href="/register" class="section-btn btn btn-default smoothScroll"> Register</a>
                                      </div>
                                 </div>
                            </div>
                       </div>
                  </div>
        </div>
   </section>

   <!-- Courses -->
   <section id="courses">
          <div class="container">
               <div class="row">

                         <div class="section-title">
                              <h2 id="changeColor">Most Recent Articles <small>Get inspired with inspiring quotes</small></h2>
                         </div>
                      
                         <div class="owl-carousel owl-theme owl-courses">
                                   <% posts.forEach(function(post) { %>
                              <div class="col-md-12 col-sm-12">
                                   <div class="item">
                                        <div class="courses-thumb">
                                             <div class="courses-top">
                                                  <div class="courses-image">
                                                       <img src="<%= post.image.secure_url %>" id="imag-size" class="img-responsive" alt="">
                                                  </div>
                                                  <div class="courses-date">
                                                       <span><i class="fa fa-calendar"></i> <%=  post.created.toDateString()  %></span>
                                                  </div>
                                             </div>

                                             <div class="courses-detail">
                                                  <h3><a href="#"><%=  post.title      %></a></h3>
                                                  <p><%=  post.desc.substring(0, 110)       %></p>
                                             </div>

                                             <div class="courses-info">
                                                  <div class="courses-author">
                                                       <img src="images/author-image1.jpg" class="img-responsive" alt="">
                                                       <span>posted by <%=  post.author.username   %></span>
                                                  </div>
                                                  <div class="courses-price">
                                                       <a href="/post/<%= post.id %>"><span>Read more</span></a>
                                                  </div>
                                             </div>
                                        </div>
                                   </div>
                              </div>
                              <%  }) %>
                         </div>
                             
          </div>
     </section>

       <!-- FEATURE -->
       <section id="feature">
          <div class="container">
               <div class="row">
          
                    <div class="col-md-4 col-sm-4">
