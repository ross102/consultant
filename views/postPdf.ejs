<% include ./partials/header  %>

<div class="container" >
        <h3  id="reg"> Upload A File</h3>
    <div class="row">
           <div class="col-md-6 col-sm-4">
                <form action="/postPdf" method="POST" enctype="multipart/form-data">
                    <div class="form-group">
					<label> Title </label>
                    <input type="text"   class="form-control" name="title" placeholder="Title of post">
					</div>
					<div class="form-group">
					<label> Choose file </label>
					<input type="file"   class="form-control" name="pdf" accept="application/pdf, application/docx">
					</div>
					<p class="help-block">Pls upload only pdf or doc file here.</p>
                    <!-- <p class="help-block">Example block-level help text here.</p> -->
                    <input type="submit" value="Create An Upload" class="btn btn-primary">
                </form>
           </div> 
    </div>
</div>


<div class="container" id="reg">
</div>

<% include ./partials/footer  %>